    "watch": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs": "^1.2.10",
    "@tensorflow/tfjs-node": "^1.2.10",
    "body-parser": "^1.19.0",
    "connect-busboy": "0.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.6-alpha.5",
    "formidable": "^1.2.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "tfjs": "^0.6.0"
